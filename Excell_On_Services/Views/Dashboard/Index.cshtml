                <div class="card ">
                    <div class="card-header">
                        <h5 class="card-title fw-bold"><strong>Services</strong></h5>
                        <p>
                            <a asp-action="AddService" asp-controller="Dashboard" class="mt-2 text-white btn btn-sm btn-success"><i class="la la-plus"></i></a>
                        </p>
                    </div>

                    <div class="card-body">
                        <div class="table-responsive">

                           
                            <table class="table align-middle text-center">
                                <thead>
                                    <tr>
                                        <th>
                                            Id
                                        </th>
                                        <th>
                                            Service
                                        </th>
                                        <th>
                                            Charges Per Month
                                        </th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in ViewBag.Services)
                                    {
                                        <tr>
                                            <td>
                                                @item.Service_Id
                                            </td>
                                            <td>
                                                @item.Service_Name
                                            </td>
                                            <td>
                                                @item.ServiceChargesPerMonth
                                            </td>
                                            <td>
                                                <a asp-action="EditService" asp-controller="Dashboard" asp-route-id="@item.Service_Id" class="btn btn-sm btn-secondary"><i class="la la-pencil"></i></a>
                                                <a asp-action="DetailService" asp-controller="Dashboard" asp-route-id="@item.Service_Id" class="btn btn-sm btn-warning text-white"><i class="la la-eye"></i></a> 
                                                <a asp-action="DeleteService" asp-controller="Dashboard" asp-route-id="@item.Service_Id" class="btn btn-sm btn-danger"><i class="la la-trash-o"></i></a>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
